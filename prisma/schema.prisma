// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Entry {
  id          Int     @id @default(autoincrement())
  questionSet Int     @default(0)
  type        String
  isCorrect   Boolean @default(false)
  text        String
}
